.zdTabBar {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 20;
  background: #ffffff;
}
.zdTabBar .ul {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.zdTabBar .ul .li {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  padding-top: 5px;
  position: relative;
}
.zdTabBar .ul .li .img {
  width: 25px;
  height: 25px;
  overflow: hidden;
  margin: 0 auto;
}
.zdTabBar .ul .li .ic {
  width: 50px;
  height: 50px;
